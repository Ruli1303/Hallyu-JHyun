@model IEnumerable<ViewModel.BiodataViewModel>

<table class="table">
    <tr>
        <th>
            NPM
        </th>
        <th>
            Nama
        </th>
        <th>
            Fakultas
        </th>
        <th>
            Jurusan
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NPM)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Faculty)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Major)
        </td>
        
    </tr>
}

</table>
